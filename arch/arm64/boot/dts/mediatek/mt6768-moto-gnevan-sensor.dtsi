&pio {
    sar_int_pin {
        sar_int_default: sar_int_default {
            /* active state */
            pinmux = <PINMUX_GPIO19__FUNC_GPIO19>;
            slew-rate = <2>;
            bias-pull-up;
        };
    };
};


&i2c5{
	Semtech_sx933x@28 {
		compatible = "Semtech,sx933x";
		reg = <0x28>;
		interrupt-parent = <&pio>;
		interrupts = <19 IRQ_TYPE_EDGE_FALLING 19 0>;
		Semtech,nirq-gpio = <&pio 19 0x00>;
		/*use PH3/4 as the reference sensor
		set it to 0xff if the ref-phases-x is not used*/
